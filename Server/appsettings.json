{
  "RegexValidator": {
    "Email": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$",
    "CNP": "/^[1-9]\\d{2}(0[1-9]|1[0-2])(0[1-9]|[12]\\d|3[01])(0[1-9]|[1-4]\\d|5[0-2]|99)(00[1-9]|0[1-9]\\d|[1-9]\\d\\d)\\d$/g",
    "Username": "^[\\w\\d_.]{4,}$",
    "Password": "^.{6,}$|^$",
    "Name": "^[A-Z][a-z]{2,}$",
    "Phone": "^\\+?\\d{10,}$",
    "Address": "^[\\w\\d\\s]{5,}$",
    "City": "^[A-Z][a-z]{2,}$",
    "Country": "^[A-Z][a-z]{2,}$",
    "PostalCode": "^\\d{6}$",
    "URL": "^https?://[\\w\\d-]+\\.[\\w\\d-]+\\.[\\w\\d-]+(/[\\w\\d-]+)*$",
    "UUID": "^[\\w\\d]{8}-[\\w\\d]{4}-[\\w\\d]{4}-[\\w\\d]{4}-[\\w\\d]{12}$",
    "CreditCard": "^\\d{4}-\\d{4}-\\d{4}-\\d{4}$",
    "Base64": "^[\\w\\d+/]+={0,2}$",
    "HTML": "^<([a-z]+)([^<]+)*(?:>(.*)<\\/\\1>|\\s+\\/>)$",
    "Domain": "^([a-z][a-z0-9-]+(\\.|-*\\.))+[a-z]{2,6}$"
  }
}
